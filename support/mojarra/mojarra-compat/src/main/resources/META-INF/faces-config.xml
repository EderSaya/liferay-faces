<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
	<name>MojarraCompat</name>
	<render-kit>
		<renderer>
			<component-family>javax.faces.OutcomeTarget</component-family>
			<renderer-type>javax.faces.Button</renderer-type>
			<renderer-class>com.liferay.faces.mojarra.renderkit.PatchedOutcomeTargetButtonRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.OutcomeTarget</component-family>
			<renderer-type>javax.faces.Link</renderer-type>
			<renderer-class>com.liferay.faces.mojarra.renderkit.PatchedOutcomeTargetLinkRenderer</renderer-class>
		</renderer>
	</render-kit>
</faces-config>
